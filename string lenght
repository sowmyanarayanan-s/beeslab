import java.util.*;
public class Hello {

    public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	String str=sc.nextLine();
	int len=str.length();
	int count=0;
	for(int i=0;i<len;i++){
	    if(str.charAt(i)==' ')
	        count++;
	    }
	
	System.out.print(len-count);
	}
}
